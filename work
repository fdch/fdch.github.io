<script>
function getContent() {
	jQuery.get('worklist', function(data){
		lines = data.split("\n");
		$.each(lines, function(n, elem) {
			$("#worklist").append("<li class=worklist onClick=\""
+ "fdLoad('content/" + elem + "')\">" + elem + "</li>");
		});
	});
}
$(getContent());
</script>
<ul id=worklist></ul>