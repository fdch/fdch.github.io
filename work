<script>
function getContent() {
	jQuery.get('content.txt', function(data){
		lines = data.split("\n");
		$.each(lines, function(n, elem) {
			$("#worklist").append('<li class=worklist onClick=' + '"' + 'fdLoadWork(content/' + elem + ')' + '"' + ' >' + elem + '</li>');
		});
	});
}
$(getContent());
</script>
<ul id=worklist>

</ul>